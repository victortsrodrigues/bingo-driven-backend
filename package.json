{
  "name": "bingo-back-end",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "npm run prisma:migrate:deploy && node dist/src/server.js",
    "dev": "ts-node-dev src/server.ts",
    "test:load-envs": "dotenv -e .env.test",
    "test": "npm run test:load-envs -- jest -- --runInBand",
    "test:ci": "npm run test:integration:migration:run && npm run test",
    "test:integration:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration": "npm run test:load-env prisma migrate deploy",
    "test:coverage": "npm run test -- --coverage",
    "prisma:migrate:deploy": "prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@types/jest": "^29.5.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.4",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.0",
    "tsx": "^4.16.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^3.1.4",
    "prisma": "^5.16.1",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2"
  }
}